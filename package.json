{
  "name": "nest-socket-template",
  "version": "0.0.1",
  "description": "Nest.js - Socket.io - Template",
  "main": "dist/main",
  "repository": "git@github.com:houseoftypescript/nest-socket-template.git",
  "author": "hieudoanm <hieumdoan@gmail.com>",
  "license": "GNU GPL 3.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "yarn docs:postman && yarn docs:html",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "docs:postman": "openapi2postmanv2 -s docs/swagger/swagger.yaml -o docs/postman/collection.json",
    "docs:html": "redocly build-docs ./docs/swagger/swagger.json --output ./public/index.html --title 'Nest.js - Socket - Template'",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@nestjs/common": "10.0.5",
    "@nestjs/core": "10.0.5",
    "@nestjs/platform-express": "10.0.5",
    "@nestjs/platform-socket.io": "10.0.5",
    "@nestjs/swagger": "7.1.1",
    "@nestjs/websockets": "10.0.5",
    "helmet": "7.0.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.1.8",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.5",
    "@redocly/cli": "1.0.0-beta.131",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.3",
    "@types/node": "20.4.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "eslint": "8.44.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "jest": "29.6.1",
    "kill-port": "2.0.1",
    "openapi-to-postmanv2": "4.15.0",
    "prettier": "3.0.0",
    "pretty-quick": "3.1.3",
    "rimraf": "5.0.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.6"
  }
}
